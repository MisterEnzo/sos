<%= form_for contact do |f| %>
  <div class='form-group'>
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class='form-group'>
    <%= f.label :email %>
    <%= f.email_field :email, class: 'form-control' %>
  </div>
  <div class="actions">
    <%= f.submit 'Add Contact', class: 'btn btn-primary' %>
  </div>
<% end %>